<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Sales::sales" title="Sales" sortOrder="20">
                    <resource id="Icare_Sales::shipment" title="Order Shipment">
                        <resource id="Icare_Sales::ready_to_ship" title="Ready to ship"/>
                        <resource id="Icare_Sales::shipment_tracking" title="Shipment Tracking"/>
                        <resource id="Icare_Sales::delivered" title="Shipment delivered"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
